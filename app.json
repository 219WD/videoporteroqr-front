{
  "expo": {
    "name": "QR Door",
    "slug": "qr-door",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "android": {
      "package": "com.yourcompany.qrdoor",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.VIBRATE"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-camera",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#7D1522"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a15c1b1f-ad22-4eb7-bddc-66266cac2cce"
      }
    },
    "owner": "jcanepa",
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
