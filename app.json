{
  "expo": {
    "name": "QR Door",
    "slug": "qr-door",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "jsEngine": "jsc",
    "sdkVersion": "54.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "android": {
      "package": "com.yourcompany.qrdoor",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.VIBRATE"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.yourcompany.qrdoor",
      "supportsTablet": true
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-asset",
      "expo-web-browser"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a15c1b1f-ad22-4eb7-bddc-66266cac2cce"
      }
    },
    "owner": "jcanepa"
  }
}
